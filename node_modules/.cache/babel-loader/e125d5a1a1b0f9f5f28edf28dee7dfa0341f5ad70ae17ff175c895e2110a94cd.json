{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport { gsap } from 'gsap';\nimport inkDroplet1 from '@/assets/inkDroplet1.svg';\nimport inkDroplet2 from '@/assets/inkDroplet2.svg';\nimport inkDroplet3 from '@/assets/inkDroplet3.svg';\nimport inkDroplet4 from '@/assets/inkDroplet4.svg';\nimport inkDroplet5 from '@/assets/inkDroplet5.svg';\nimport inkDroplet6 from '@/assets/inkDroplet6.svg';\nexport default {\n  name: 'LandingPage',\n  data() {\n    return {\n      droplets: Array(100).fill(0),\n      // Create an array with 100 entries\n      inkDroplets: [inkDroplet1, inkDroplet2, inkDroplet3, inkDroplet4, inkDroplet5, inkDroplet6]\n    };\n  },\n  methods: {\n    startJournaling() {\n      this.$router.push('/journaling');\n    },\n    getRandomDroplet() {\n      return this.inkDroplets[Math.floor(Math.random() * this.inkDroplets.length)];\n    },\n    getDropletStyle() {\n      const size = Math.random() * 80 + 20; // Random size between 20px and 100px\n      const left = Math.random() * 100; // Random left position (0% - 100%)\n      const top = Math.random() * 100; // Random top position (0% - 100%)\n      return {\n        width: `${size}px`,\n        height: `${size}px`,\n        left: `${left}%`,\n        top: `${top}%`\n      };\n    }\n  },\n  mounted() {\n    // Optional: Add animation to droplets on mount\n    this.animateDroplets();\n  },\n  animateDroplets() {\n    const droplets = this.$refs.hero.children;\n    Array.from(droplets).forEach((droplet, index) => {\n      gsap.fromTo(droplet, {\n        scale: 0.8,\n        opacity: 0,\n        y: -30\n      }, {\n        scale: 1,\n        opacity: 1,\n        y: 0,\n        duration: 1.2,\n        delay: index * 0.1,\n        ease: 'power2.out'\n      });\n    });\n  }\n};","map":{"version":3,"names":["gsap","inkDroplet1","inkDroplet2","inkDroplet3","inkDroplet4","inkDroplet5","inkDroplet6","name","data","droplets","Array","fill","inkDroplets","methods","startJournaling","$router","push","getRandomDroplet","Math","floor","random","length","getDropletStyle","size","left","top","width","height","mounted","animateDroplets","$refs","hero","children","from","forEach","droplet","index","fromTo","scale","opacity","y","duration","delay","ease"],"sources":["C:\\Users\\d.Brown\\Desktop\\d.brown\\portfolio\\src\\components\\LandingPage.vue"],"sourcesContent":["<template>\r\n    <section class=\"landing-page\">\r\n      <div class=\"hero\" ref=\"hero\">\r\n        <img\r\n          v-for=\"(droplet, index) in droplets\"\r\n          :key=\"index\"\r\n          class=\"ink-droplet\"\r\n          :src=\"getRandomDroplet()\"\r\n          :style=\"getDropletStyle()\"\r\n        />\r\n      </div>\r\n      <h1>Capture Your Thoughts</h1>\r\n      <button @click=\"startJournaling\">Start Your Journaling Journey</button>\r\n    </section>\r\n  </template>\r\n  \r\n  <script>\r\n  import { gsap } from 'gsap';\r\n  import inkDroplet1 from '@/assets/inkDroplet1.svg';\r\n  import inkDroplet2 from '@/assets/inkDroplet2.svg';\r\n  import inkDroplet3 from '@/assets/inkDroplet3.svg';\r\n  import inkDroplet4 from '@/assets/inkDroplet4.svg';\r\n  import inkDroplet5 from '@/assets/inkDroplet5.svg';\r\n  import inkDroplet6 from '@/assets/inkDroplet6.svg';\r\n  \r\n  export default {\r\n    name: 'LandingPage',\r\n    data() {\r\n      return {\r\n        droplets: Array(100).fill(0), // Create an array with 100 entries\r\n        inkDroplets: [\r\n          inkDroplet1,\r\n          inkDroplet2,\r\n          inkDroplet3,\r\n          inkDroplet4,\r\n          inkDroplet5,\r\n          inkDroplet6,\r\n        ],\r\n      };\r\n    },\r\n    methods: {\r\n      startJournaling() {\r\n        this.$router.push('/journaling');\r\n      },\r\n      getRandomDroplet() {\r\n        return this.inkDroplets[Math.floor(Math.random() * this.inkDroplets.length)];\r\n      },\r\n      getDropletStyle() {\r\n        const size = Math.random() * 80 + 20; // Random size between 20px and 100px\r\n        const left = Math.random() * 100; // Random left position (0% - 100%)\r\n        const top = Math.random() * 100; // Random top position (0% - 100%)\r\n        return {\r\n          width: `${size}px`,\r\n          height: `${size}px`,\r\n          left: `${left}%`,\r\n          top: `${top}%`,\r\n        };\r\n      },\r\n    },\r\n    mounted() {\r\n      // Optional: Add animation to droplets on mount\r\n      this.animateDroplets();\r\n    },\r\n    animateDroplets() {\r\n      const droplets = this.$refs.hero.children;\r\n      Array.from(droplets).forEach((droplet, index) => {\r\n        gsap.fromTo(droplet, {\r\n          scale: 0.8,\r\n          opacity: 0,\r\n          y: -30,\r\n        }, {\r\n          scale: 1,\r\n          opacity: 1,\r\n          y: 0,\r\n          duration: 1.2,\r\n          delay: index * 0.1,\r\n          ease: 'power2.out',\r\n        });\r\n      });\r\n    },\r\n  };\r\n  </script>\r\n  \r\n  <style scoped>\r\n  .landing-page {\r\n    text-align: center;\r\n    padding: 50px;\r\n    background: url('@/assets/parchment.jpg') no-repeat center center;\r\n    background-size: cover;\r\n    height: 100vh;\r\n    position: relative;\r\n  }\r\n  \r\n  .hero {\r\n    position: relative;\r\n    height: 100%;\r\n  }\r\n  \r\n  .ink-droplet {\r\n    position: absolute;\r\n    opacity: 0; /* Start invisible */\r\n    transition: opacity 0.3s ease; /* Smooth transition for opacity */\r\n  }\r\n  </style>\r\n  "],"mappings":";AAiBE,SAASA,IAAG,QAAS,MAAM;AAC3B,OAAOC,WAAU,MAAO,0BAA0B;AAClD,OAAOC,WAAU,MAAO,0BAA0B;AAClD,OAAOC,WAAU,MAAO,0BAA0B;AAClD,OAAOC,WAAU,MAAO,0BAA0B;AAClD,OAAOC,WAAU,MAAO,0BAA0B;AAClD,OAAOC,WAAU,MAAO,0BAA0B;AAElD,eAAe;EACbC,IAAI,EAAE,aAAa;EACnBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,QAAQ,EAAEC,KAAK,CAAC,GAAG,CAAC,CAACC,IAAI,CAAC,CAAC,CAAC;MAAE;MAC9BC,WAAW,EAAE,CACXX,WAAW,EACXC,WAAW,EACXC,WAAW,EACXC,WAAW,EACXC,WAAW,EACXC,WAAW;IAEf,CAAC;EACH,CAAC;EACDO,OAAO,EAAE;IACPC,eAAeA,CAAA,EAAG;MAChB,IAAI,CAACC,OAAO,CAACC,IAAI,CAAC,a