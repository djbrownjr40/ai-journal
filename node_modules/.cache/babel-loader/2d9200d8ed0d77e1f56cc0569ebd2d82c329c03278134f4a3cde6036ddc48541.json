{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport { gsap } from 'gsap';\nimport inkDroplet1 from '@/assets/inkDroplet1.svg';\nimport inkDroplet2 from '@/assets/inkDroplet2.svg';\nimport inkDroplet3 from '@/assets/inkDroplet3.svg';\nimport inkDroplet4 from '@/assets/inkDroplet4.svg';\nexport default {\n  name: 'LandingPage',\n  data() {\n    return {\n      inkDroplet1,\n      inkDroplet2,\n      inkDroplet3,\n      inkDroplet4\n    };\n  },\n  methods: {\n    startJournaling() {\n      this.$router.push('/journaling');\n    },\n    animateDroplets() {\n      const droplets = [this.$refs.droplet1, this.$refs.droplet2, this.$refs.droplet3, this.$refs.droplet4];\n      droplets.forEach((droplet, index) => {\n        gsap.fromTo(droplet, {\n          scale: 0.8,\n          // Start slightly smaller\n          opacity: 0,\n          // Start invisible\n          y: -30 // Start above the final position\n        }, {\n          scale: 1.1,\n          // Scale up to simulate splash\n          opacity: 1,\n          // Fully visible\n          y: 0,\n          // Final position\n          duration: 1.2,\n          delay: index * 0.6,\n          // Stagger the animations\n          ease: 'power2.out',\n          // Easing for a smooth animation\n          onComplete: () => {\n            gsap.to(droplet, {\n              scale: 1,\n              // Scale back to normal\n              duration: 0.5,\n              ease: 'power2.in' // Easing for settling effect\n            });\n          }\n        });\n      });\n    }\n  },\n  mounted() {\n    this.animateDroplets();\n  }\n};","map":{"version":3,"names":["gsap","inkDroplet1","inkDroplet2","inkDroplet3","inkDroplet4","name","data","methods","startJournaling","$router","push","animateDroplets","droplets","$refs","droplet1","droplet2","droplet3","droplet4","forEach","droplet","index","fromTo","scale","opacity","y","duration","delay","ease","onComplete","to","mounted"],"sources":["C:\\Users\\d.Brown\\Desktop\\d.brown\\portfolio\\src\\components\\LandingPage.vue"],"sourcesContent":["<template>\r\n    <section class=\"landing-page\">\r\n      <div class=\"hero\">\r\n        <img ref=\"droplet1\" class=\"ink-droplet\" :src=\"inkDroplet1\" />\r\n        <img ref=\"droplet2\" class=\"ink-droplet\" :src=\"inkDroplet2\" />\r\n        <img ref=\"droplet3\" class=\"ink-droplet\" :src=\"inkDroplet3\" />\r\n        <img ref=\"droplet4\" class=\"ink-droplet\" :src=\"inkDroplet4\" />\r\n      </div>\r\n      <h1>Capture Your Thoughts</h1>\r\n      <button @click=\"startJournaling\">Start Your Journaling Journey</button>\r\n    </section>\r\n  </template>\r\n  \r\n  <script>\r\n  import { gsap } from 'gsap';\r\n  import inkDroplet1 from '@/assets/inkDroplet1.svg';\r\n  import inkDroplet2 from '@/assets/inkDroplet2.svg';\r\n  import inkDroplet3 from '@/assets/inkDroplet3.svg';\r\n  import inkDroplet4 from '@/assets/inkDroplet4.svg';\r\n  \r\n  export default {\r\n    name: 'LandingPage',\r\n    data() {\r\n      return {\r\n        inkDroplet1,\r\n        inkDroplet2,\r\n        inkDroplet3,\r\n        inkDroplet4,\r\n      };\r\n    },\r\n    methods: {\r\n    startJournaling() {\r\n      this.$router.push('/journaling');\r\n    },\r\n    animateDroplets() {\r\n      const droplets = [this.$refs.droplet1, this.$refs.droplet2, this.$refs.droplet3, this.$refs.droplet4];\r\n\r\n      droplets.forEach((droplet, index) => {\r\n        gsap.fromTo(droplet, {\r\n          scale: 0.8, // Start slightly smaller\r\n          opacity: 0, // Start invisible\r\n          y: -30, // Start above the final position\r\n        }, {\r\n          scale: 1.1, // Scale up to simulate splash\r\n          opacity: 1, // Fully visible\r\n          y: 0, // Final position\r\n          duration: 1.2,\r\n          delay: index * 0.6, // Stagger the animations\r\n          ease: 'power2.out', // Easing for a smooth animation\r\n          onComplete: () => {\r\n            gsap.to(droplet, {\r\n              scale: 1, // Scale back to normal\r\n              duration: 0.5,\r\n              ease: 'power2.in', // Easing for settling effect\r\n            });\r\n          }\r\n        });\r\n      });\r\n    }\r\n  },\r\n  mounted() {\r\n    this.animateDroplets();\r\n  }\r\n}\r\n</script>\r\n  \r\n  <style scoped>\r\n  .landing-page {\r\n    text-align: center;\r\n    padding: 50px;\r\n    background: url('@/assets/parchment.jpg') no-repeat center center;\r\n    background-size: cover;\r\n    height: 100vh;\r\n    position: relative;\r\n  }\r\n  \r\n  .hero {\r\n    position: relative;\r\n    height: 100%; /* Ensure the hero section uses full height */\r\n  }\r\n  \r\n  .ink-droplet {\r\n    position: absolute;\r\n    opacity: 0; /* Start invisible */\r\n    width: 40px; /* Adjust size as needed */\r\n    height: auto; /* Maintain aspect ratio */\r\n  }\r\n  \r\n  /* Position droplets */\r\n  .ink-droplet:nth-child(1) {\r\n    left: 10%;\r\n    bottom: 10px;\r\n  }\r\n  \r\n  .ink-droplet:nth-child(2) {\r\n    left: 50%;\r\n    bottom: 25%;\r\n    transform: translate(-50%, 50%);\r\n  }\r\n  \r\n  .ink-droplet:nth-child(3) {\r\n    right: 10%;\r\n    top: 10%;\r\n  }\r\n  \r\n  .ink-droplet:nth-child(4) {\r\n    right: 55%;\r\n    top: 20%;  }\r\n  </style>\r\n  "],"mappings":";AAcE,SAASA,IAAG,QAAS,MAAM;AAC3B,OAAOC,WAAU,MAAO,0BAA0B;AAClD,OAAOC,WAAU,MAAO,0BAA0B;AAClD,OAAOC,WAAU,MAAO,0BAA0B;AAClD,OAAOC,WAAU,MAAO,0BAA0B;AAElD,eAAe;EACbC,IAAI,EAAE,aAAa;EACnBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLL,WAAW;MACXC,WAAW;MACXC,WAAW;MACXC;IACF,CAAC;EACH,CAAC;EACDG,OAAO,EAAE;IACTC,eAAeA,CAAA,EAAG;MAChB,IAAI,CAACC,OAAO,CAACC,IAAI,CAAC,aAAa,CAAC;IAClC,CAAC;IACDC,eAAeA,CAAA,EAAG;MAChB,MAAMC,QAAO,GAAI,CAAC,IAAI,CAACC,KAAK,CAACC,QAAQ,EAAE,IAAI,CAACD,KAAK,CAACE,QAAQ,EAAE,IAAI,CAACF,KAAK,CAACG,QAAQ,EAAE,IAAI,CAACH,KAAK,CAACI,QAAQ,CAAC;MAErGL,QAAQ,CAACM,OAAO,CAAC,CAACC,OAAO,EAAEC,KAAK,KAAK;QACnCpB,IAAI,CAACqB,MAAM,CAACF,OAAO,EAAE;UACnBG,KAAK,EAAE,GAAG;UAAE;UACZC,OAAO,EAAE,CAAC;UAAE;UACZC,CAAC,EAAE,